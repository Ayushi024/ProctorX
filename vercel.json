{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",  
      "use": "@vercel/static-build"
    }
  ],
  "env": {
    "NODE_VERSION": "20.x"
  },
  "installCommand": "cd client && npm install --legacy-peer-deps && npm install react-scripts",
  "buildCommand": "cd client && npm run build",
  "devCommand": "npm run dev"
}
